{
  "BasePath": "Logs",
  "EnsureBasePath": true,
  "FileEncoding": "utf-8",
  "FileNameMappings": {
    "MyApp.SomeClass": "someclass.log",
    "Default": "default.log"
  },
  "DateFormat": "yyyyMMdd",
  "CounterFormat": "000",
  "MaxFileSize": 10485760,
  "TextBuilder": "MyApp.CustomLogEntryTextBuilder, MyApp",
  "LogLevel": {
    "MyApp": "Information",
    "Default": "Warning"
  },
  "IncludeScopes": true,
  "MaxQueueSize": 100,
  "app": {
    "workingIps": [ "95.97.52.106", "95.97.52.106" ]
  }
}
